#: ../json-glib/json-gvariant.c:1281
msgid "Invalid GVariant signature"
msgstr "ଅବୈଧ GVariant ହସ୍ତାକ୍ଷର"

#: ../json-glib/json-gvariant.c:1326
msgid "JSON data is empty"
msgstr "JSON ତଥ୍ୟ ନାହିଁ"

